{
  "name": "achat",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test-cover-server": "$(npm bin)/istanbul cover $(npm bin)/_mocha --report lcovonly --hook-run-in-context -x *.test.js -- --reporter dot --opts .mochaconfig",
    "test-cover-client": "$(npm bin)/karma start --reporters progress,coverage",
    "test-cover": "npm run test-cover-server && npm run test-cover-client",
    "test-server": "$(npm bin)/mocha --opts .mochaconfig",
    "test-client": "$(npm bin)/karma start --reporters progress",
    "test": "npm run test-client --silent& npm run test-server --silent& wait",
    "coverall": "$(npm bin)/istanbul report lcovonly && cat ./coverage/lcov.info | $(npm bin)/coveralls",
    "postinstall": "$(npm bin)/bower install"
  },
  "dependencies": {
    "bluebird": "^2.9.30",
    "body-parser": "~1.12.4",
    "bower": "^1.4.1",
    "config-chain": "^1.1.9",
    "connect-redis": "^2.3.0",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.12.4",
    "express-session": "^1.11.3",
    "hbs": "~2.9.0",
    "hiredis": "^0.4.0",
    "lodash": "^3.9.3",
    "marionette": "0.0.0",
    "morgan": "~1.5.3",
    "optimist": "^0.6.1",
    "redis": "^0.12.1",
    "serve-favicon": "~2.2.1",
    "socket.io": "^1.3.5",
    "stylus": "0.42.3"
  },
  "devDependencies": {
    "amd-optimize": "^0.4.3",
    "chai": "^3.0.0",
    "chai-as-promised": "^5.1.0",
    "coveralls": "^2.11.2",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.5.2",
    "gulp-requirejs-optimize": "^0.1.3",
    "istanbul": "^0.3.15",
    "karma": "^0.12.37",
    "karma-chai-plugins": "^0.6.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-coverage": "^0.4.2",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-requirejs": "^0.2.2",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.2",
    "phantomjs": "^1.9.17",
    "require-stub": "^2.2.4",
    "requirejs": "^2.1.18",
    "sinon": "^1.15.3",
    "sinon-chai": "^2.8.0",
    "socket.io-client": "^1.3.5"
  }
}
